# Option Quant Trade System Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Futu OpenAPI Configuration (optional, defaults shown)
# Note: Futu only provides HK market data for this account
FUTU_HOST=127.0.0.1
FUTU_PORT=11111

# IBKR TWS API Configuration (for US market data)
# Requires TWS or IB Gateway running locally
# TWS ports: Live=7496, Paper=7497
# IB Gateway ports: Live=4001, Paper=4002
IBKR_HOST=127.0.0.1
IBKR_PORT=7497
IBKR_CLIENT_ID=1

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
